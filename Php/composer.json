{
    "name": "skilla/kataphp",
    "description": "a template to start making a kata with PHP and PhpUnit",
    "type": "project",
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "license": "GNU",
    "authors": [
        {
            "name": "Sergio Zambrano",
            "email": "sergio.zambrano@gmail.com"
        }
    ],
    "require": {},
    "autoload": {
        "psr-4": {
            "Kata\\Test\\": "tests/",
            "Kata\\Main\\": "src/"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "bindir": "bin",
        "sort-packages": true
    }
}
